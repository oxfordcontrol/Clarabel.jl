{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "attach",
            "name": "Attach",
            "pid": "14368",
            "stopOnEntry": true
        },

        {
            "type": "julia",
            "request": "launch",
            "name": "Julia interactive terminal launch",
            "program": "${workspaceFolder}/julia",
            "stopOnEntry": true
        },

        {
            "type": "julia",
            "request": "launch",
            "name": "QP Rust debug",
            "program": "${workspaceFolder}/examples/example_QP_debug.jl",
            "stopOnEntry": false
        },
        {
            "type": "julia",
            "request": "launch",
            "name": "QP box Rust debug",
            "program": "${workspaceFolder}/examples/example_big_box.jl",
            "stopOnEntry": false
        },
        {
            "type": "julia",
            "request": "launch",
            "name": "SOCP Rust debug",
            "program": "${workspaceFolder}/examples/example_SOCP_debug.jl",
            "stopOnEntry": false
        },
        {
            "type": "julia",
            "request": "launch",
            "name": "Solver Test Suite",
            "program": "${workspaceFolder}/test/runtests.jl",
            "stopOnEntry": false
        },
        {
            "type": "julia",
            "request": "launch",
            "name": "Run active Julia file",
            "program": "${file}",
            "stopOnEntry": false,
            "cwd": "${workspaceFolder}",
            "juliaEnv": "${command:activeJuliaEnvironment}"
        }
    ]
}